{
  "name": "@de/types",
  "version": "0.1.0",
  "description": "TypeScript type definitions for DE frontends",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:types": "tsc --noEmit --skipLibCheck",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [
    "typescript",
    "types",
    "definitions",
    "de-frontend"
  ]
}